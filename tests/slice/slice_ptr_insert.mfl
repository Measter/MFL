proc entry [u64, u8**] to [] {
    var slice: std::mem::MMap(u8)

    // We'll just assume we're being called correctly.
    swap drop 

    1+@@ '0'- // This is our index

    3 std::mem::alloc(u8) std::Result::unwrap_ok // We'll just assume allocation succeeds
    slice!
    
    1 slice rot(3<1)
    insd
}