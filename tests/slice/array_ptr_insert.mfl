proc entry [u64 u8&&] to [] is
    memory arr is u8[3] end

    // We'll just assume we're being called correctly.
    swap drop 

    1+@@ '0'- // This is our index

    0 0 0 pack(3) arr!
    
    1 arr rot(3<1)
    insd
end