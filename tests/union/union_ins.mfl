struct Foo {
    field a u16,
    field b u16,
}

union Bar {
    field foo Foo,
    field int u32,
}

proc entry [] to [] {
    memory BAR { Bar }

    0xDEADBEEF(u32) BAR insd(int)
    
    BAR xtrd(foo) unpack
    std::term::write_ui_ln
    std::term::write_ui_ln

    sizeof(Bar)
    std::term::write_ui_ln
}